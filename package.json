{
  "name": "onnx-ocr-js",
  "version": "0.0.6",
  "description": "ONNX-based OCR system for Node.js and Browser (PaddleOCR compatible)",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:playwright": "pnpm exec playwright test",
    "types:esm": "tsc -p tsconfig.esm.json",
    "types:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "pnpm run types:esm && pnpm run types:cjs",
    "build:bundle": "node esbuild.config.mjs",
    "build": "npm run build:types && npm run build:bundle",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "onnx",
    "ocr",
    "paddleocr",
    "computer-vision",
    "nodejs",
    "browser"
  ],
  "author": "SotaTne <sota@sotatne.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/SotaTne/OnnxOcrJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/SotaTne/OnnxOcrJS.git"
  },
  "bugs": {
    "url": "https://github.com/SotaTne/OnnxOcrJS/issues"
  },
  "files": [
    "dist",
    "models"
  ],
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@techstark/opencv-js": "4.11.0-release.1",
    "@types/ndarray": "^1.0.14",
    "@types/ndarray-ops": "^1.2.7",
    "@types/node": "^24.6.2",
    "@types/serve-handler": "^6.1.4",
    "esbuild": "^0.25.10",
    "jimp": "^1.6.0",
    "onnxruntime-node": "^1.23.0",
    "onnxruntime-react-native": "^1.23.0",
    "onnxruntime-web": "^1.23.0",
    "prettier": "^3.6.2",
    "serve-handler": "^6.1.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "blakejs": "^1.2.1",
    "js-angusj-clipper": "^1.3.1",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2"
  }
}
